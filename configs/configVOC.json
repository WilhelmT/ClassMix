{
    "dataset": "pascal_voc",
    "ignore_label": 255,
    "model": "DeepLab",
    "pretrained": "coco",
    "seed": 1,
    "training": {
        "batch_size": 10,
        "data": {
            "crop": false,
            "input_size": "321,321",
            "labeled_samples": 1323,
            "scale": true,
            "split_id_list": 0
        },
        "learning_rate": 0.00025,
        "lr_schedule": "Poly",
        "lr_schedule_power": 0.9,
        "momentum": 0.9,
        "num_iterations": 40000,
        "num_workers": 4,
        "optimizer": "SGD",
        "unlabeled": {
            "blur": false,
            "color_jitter": false,
            "consistency_loss": "CE",
            "consistency_weight": 1,
            "flip": false,
            "pixel_weight": "threshold_uniform",
            "mix_mask": "class",
            "train_unlabeled": true
        },
        "use_sync_batchnorm": true,
        "weight_decay": 0.0005
    },
    "utils": {
        "checkpoint_dir": "../saved/DeepLabv2",
        "log_per_iter": 200,
        "save_best_model": false,
        "save_checkpoint_every": 1000,
        "tensorboard": true,
        "val_per_iter": 5000
    }
}
